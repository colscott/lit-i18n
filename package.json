  "bugs": {
    "url": "https://github.com/colscott/lit-i18n/issues"
  },
  "homepage": "https://github.com/colscott/lit-i18n#readme",
  "dependencies": {
    "lit-html": "^1.1.1"
  },
  "devDependencies": {
    "i18next": "^17.0.6",
    "dev-lib-colscott": "^1.1.7",
    "express": "^4.17.1"
  },
  "peerDependencies": {
    "i18next": "^17.0.6"
  }
